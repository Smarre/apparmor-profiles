# Profile for git-remote-http

#include <tunables/global>

# git-remote-http should be inherited; this profile is only a fallaback option.

/usr/lib/git/git-remote-http flags=(complain) {
  #include <abstractions/base>
  #include <abstractions/nameservice>
  #include <abstractions/openssl>

  /usr/lib/git/git Px,
  /usr/lib/git/git-remote-http mr,

  # TODO: why?
  owner /** r,

}

#include <tunables/global>

/usr/bin/akonadi_davgroupware_resource flags=(complain) {
  #include <abstractions/akonadi_resource_base>
  #include <abstractions/nameservice>
  #include <abstractions/smar_kde>

  signal receive set=kill peer=/usr/bin/akonadi_control,
  signal receive set=term peer=/usr/bin/akonadi_control,
  signal send set=term peer=/usr/bin/kdeinit5,
  signal send set=term peer=unconfined,

  /etc/xdg/accept-languages.codes r,
  /etc/xdg/menus/ r,
  /etc/xdg/menus/applications-merged/ r,
  /usr/bin/akonadi_davgroupware_resource mr,
  /usr/bin/kdeinit5 Px,
  /usr/bin/kdeinit5 mr,
  /usr/share/akonadi/plugins/serializer/ r,
  /usr/share/akonadi/plugins/serializer/* r,
  owner /run/user/*/akonadi_davgroupware_resource_*.slave-socket rw,
  owner @{HOME}/.cache/* r,
  owner @{HOME}/.cache/fontconfig/* r,
  owner @{HOME}/.config/.* rwk,
  owner @{HOME}/.config/akonadi/* k,
  owner @{HOME}/.config/akonadi/* rw,
  owner @{HOME}/.config/akonadi/akonadiconnectionrc r,
  owner @{HOME}/.config/akonadi_davgroupware_resource* rw,
  owner @{HOME}/.config/akonadi_davgroupware_resource*.lock wk,
  owner @{HOME}/.config/kwalletrc r,
  owner @{HOME}/.config/menus/ r,
  owner @{HOME}/.config/menus/applications-merged/ r,
  owner @{HOME}/.fonts.conf r,
  owner @{HOME}/.fonts/ r,
  owner @{HOME}/.local/share/akonadi-davgroupware/ w,
  owner @{HOME}/.local/share/akonadi-davgroupware/* rw,

}

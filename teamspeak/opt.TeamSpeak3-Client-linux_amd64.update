# Program for updating TeamSpeak3.

#include <tunables/global>

# This profile mostly exists just to prevent updates to happen. That leaks
# information unnecessarily and should not be preferred, as distribution
# packages should be used instead.

profile teamspeak_update /opt/TeamSpeak3-Client-linux_amd64/update {
  #include <abstractions/base>

  /opt/TeamSpeak3-Client-linux_amd64/update mr,
  /opt/TeamSpeak3-Client-linux_amd64/update: mr,
}
